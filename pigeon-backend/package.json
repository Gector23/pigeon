{
  "name": "pigeon-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.14.2",
    "npm": "^9.5.0"
  },
  "scripts": {
    "prebuild": "rimraf build/",
    "build": "npx tsc",
    "prestart": "npm run build",
    "start": "node build/index.js",
